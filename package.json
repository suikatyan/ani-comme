{
  "name": "ani-comme",
  "version": "0.1.0",
  "private": true,
  "author": "suikatyan",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "animate.css": "^4.1.0",
    "core-js": "^3.6.5",
    "js-base64": "^2.5.2",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.4.4",
    "@vue/cli-plugin-eslint": "^4.4.4",
    "@vue/cli-plugin-router": "^4.4.4",
    "@vue/cli-service": "^4.4.4",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.2.2",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/recommended",
      "eslint:all"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "quotes": [
        "warn",
        "single"
      ],
      "indent": [
        "warn",
        2
      ],
      "sort-keys": "off",
      "no-console": "off",
      "max-len": "off",
      "max-lines": "off",
      "no-magic-numbers": "off",
      "sort-vars": "off",
      "no-inline-comments": "off",
      "line-comment-position": "off",
      "id-length": "off",
      "max-lines-per-function": "off",
      "semi": [
        "warn",
        "never"
      ],
      "padded-blocks": [
        "error",
        "never"
      ],
      "space-before-function-paren": [
        "error",
        {
          "anonymous": "never",
          "named": "never",
          "asyncArrow": "always"
        }
      ],
      "quote-props": [
        "error",
        "as-needed"
      ],
      "comma-dangle": [
        "error",
        {
          "arrays": "always-multiline",
          "objects": "always-multiline",
          "imports": "always-multiline",
          "exports": "always-multiline",
          "functions": "always-multiline"
        }
      ],
      "function-call-argument-newline": [
        "error",
        "consistent"
      ],
      "one-var": "off",
      "no-ternary": "off",
      "multiline-ternary": "off",
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "lines-between-class-members": "off",
      "no-plusplus": "off",
      "func-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
